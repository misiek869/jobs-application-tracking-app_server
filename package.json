		"@types/jsonwebtoken": "^9.0.10",
		"@types/morgan": "^1.9.10",
		"bcrypt": "^6.0.0",
		"bcryptjs": "^3.0.2",
		"concurrently": "^8.0.1",
		"cookie-parser": "^1.4.7",
