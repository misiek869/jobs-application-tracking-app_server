{
	"name": "jobs-application-tracking-app",
	"version": "1.0.0",
	"type": "module",
	"main": "dist/server.js",
	"scripts": {
		"server": "nodemon --watch src --ext ts,js --exec \"node --loader ts-node/esm ./src/server.ts\"",
		"client": "cd client && npm run dev",
		"dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
